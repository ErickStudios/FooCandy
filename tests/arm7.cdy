safe jumps _start

candy Lib::Math

Void fn _start() {
    unsafe "
       ldr x29, =stack_top   // usar x29 temporalmente
        mov sp, x29           // inicializar stack pointer
    "
    r1=2 r2=6 r1<- r2<-
    Lib::Math::SimplePow()
    return
}

unsafe "
// reserva 1KB de stack
.section .bss
.align 16
stack_space:
   .skip 1024
stack_top:
"